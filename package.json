{
  "name": "node-starter",
  "version": "1.0.1",
  "description": "",
  "main": "build/index.js",
  "private": true,
  "license": "MPL-2.0",
  "engines": {
    "node": ">=12",
    "npm": ">=6.9"
  },
  "scripts": {
    "clean": "npm run build:clean",
    "copy-assets": "copyfiles docs/* public/* build/",
    "build": "npm run copy-assets && npm run tsc",
    "build:watch": "npm run copy-assets && npm run tsc -- -w",
    "build:image": "./scripts/build_image.sh",
    "build:clean": "rimraf build",
    "tsc": "tsc",
    "start": "npm run db:migrate && node build/index.js",
    "start:dev": "nodemon build/index.js",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "lint": "esw *.ts server/**/*.ts config/**/*.ts --color",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test npm run db:migrate && NODE_ENV=test mocha --ui bdd --reporter spec --colors build/index.test.js build --recursive",
    "test:watch": "NODE_ENV=test npm run db:migrate && NODE_ENV=test npm test -- --watch",
    "test:coverage": "NODE_ENV=test npm run db:migrate && cross-env NODE_ENV=test istanbul cover _mocha -- --ui bdd --reporter spec --colors build/server --recursive --exit",
    "test:coverage:html": "NODE_ENV=test npm run db:migrate && cross-env NODE_ENV=test istanbul cover --reporter=html _mocha -- --ui bdd --reporter spec --colors build/server --recursive --exit",
    "cloc": "cloc config db scripts/ server/ index.ts index.route.ts index.test.ts"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "camelcase-keys": "https://github.com/mskec/camelcase-keys#d24a316",
    "celebrate": "^13.0.4",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-winston": "3.4.0",
    "helmet": "3.23.3",
    "http-status": "1.4.2",
    "jsonwebtoken": "8.5.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.20",
    "lru-cache": "^5.1.1",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "pg": "8.5.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^3.4.0",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/bluebird-global": "^3.5.12",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.1.4",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^14.14.7",
    "@types/validator": "^10.11.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "chai": "4.2.0",
    "chai-http": "^4.3.0",
    "copyfiles": "^2.4.0",
    "cross-env": "5.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-watch": "^6.0.1",
    "faker": "^5.1.0",
    "husky": "4.3.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "7.2.0",
    "mock-require": "^3.0.3",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm test"
    }
  }
}

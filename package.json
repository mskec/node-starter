{
  "name": "node-starter",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "private": true,
  "license": "MPL-2.0",
  "engines": {
    "node": ">=12.6.0",
    "npm": ">=6.9.0"
  },
  "type": "module",
  "scripts": {
    "clean": "npm run build:clean",
    "copy-assets": "copyfiles docs/* public/* build/",
    "build": "npm run copy-assets && npm run tsc",
    "build:watch": "npm run copy-assets && npm run tsc -- -w",
    "build:image": "./scripts/build_image.sh",
    "build:clean": "rimraf build",
    "tsc": "tsc",
    "start": "npm run db:migrate && node build/index.js",
    "start:dev": "nodemon build/index.js",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "prepush": "npm run lint && npm test",
    "lint": "esw *.ts server/**/*.ts config/**/*.ts --color",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test npm run db:migrate && NODE_ENV=test mocha --ui bdd --reporter spec --colors build/index.test.js build --recursive",
    "test:watch": "NODE_ENV=test npm run db:migrate && NODE_ENV=test npm test -- --watch",
    "test:coverage": "NODE_ENV=test npm run db:migrate && cross-env NODE_ENV=test istanbul cover _mocha -- --ui bdd --reporter spec --colors build/server --recursive --exit",
    "test:coverage:html": "NODE_ENV=test npm run db:migrate && cross-env NODE_ENV=test istanbul cover --reporter=html _mocha -- --ui bdd --reporter spec --colors build/server --recursive --exit",
    "cloc": "cloc config db scripts/ server/ index.ts index.route.ts index.test.ts"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "dependencies": {
    "@hapi/joi": "^17.0.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "camelcase-keys": "https://github.com/mskec/camelcase-keys#d24a316",
    "celebrate": "^11.0.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "dotenv": "^8.0.0",
    "express": "4.17.1",
    "express-winston": "3.2.0",
    "helmet": "3.18.0",
    "http-status": "1.3.2",
    "jsonwebtoken": "8.5.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "pg": "7.11.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sequelize": "^5.8.12",
    "sequelize-cli": "^5.5.0",
    "swagger-ui-express": "^4.0.6",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "yamljs": "^0.3.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/bluebird-global": "^3.5.12",
    "@types/express": "^4.17.2",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.9",
    "@types/validator": "^10.11.3",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "babel-eslint": "^10.0.2",
    "chai": "4.2.0",
    "chai-http": "^4.3.0",
    "copyfiles": "^2.1.1",
    "cross-env": "5.2.0",
    "csv": "^5.1.1",
    "eslint": "^6.0.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-watch": "^6.0.0",
    "faker": "^4.1.0",
    "husky": "2.5.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "7.0.0",
    "mock-require": "^3.0.3",
    "nodemon": "1.19.1",
    "rimraf": "^3.0.0",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "typescript": "^3.7.2"
  }
}
